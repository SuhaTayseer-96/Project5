    <style>
        /* General Styles */

        /* Slider Styles */
        .slider-wrap {
            position: relative;
        }

        .slider_item_container {
            position: relative;
            color: #fff;
        }

        .background-video {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .slider-content {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            text-align: center;
        }

        /* Event Layout */
        .event-layout-1 {
            border: 1px solid #ddd;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            background-color: #fff;
            overflow: hidden;
        }

        .event-img img {
            width: 100%;
            height: auto;
        }

        .event-info {
            padding: 20px;
        }

        .event-date {
            color: #555;
            font-size: 14px;
            margin-bottom: 10px;
        }

        .event-info h4 a {
            color: #333;
            font-size: 18px;
            font-weight: bold;
            text-decoration: none;
        }

        .event-exceprt {
            color: #666;
            font-size: 14px;
            margin: 10px 0;
        }

        .event-time {
            color: #888;
            font-size: 14px;
        }

            .event-time a {
                color: #888;
                text-decoration: none;
                margin-right: 10px;
            }

                .event-time a i {
                    margin-right: 5px;
                }

        /* Highlight Carousel */
        .highlight-layout-1 {
            border-radius: 8px;
            overflow: hidden;
            background-color: #fff;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        }

            .highlight-layout-1 figure img {
                width: 100%;
                height: auto;
            }

            .highlight-layout-1 h5 a {
                color: #333;
                font-size: 18px;
                text-decoration: none;
            }

            .highlight-layout-1 div {
                color: #666;
                font-size: 14px;
                padding: 10px;
            }

            .highlight-layout-1 .btn {
                margin-top: 10px;
            }

        /* Counter Block */
        .counter-block-1 {
            padding: 20px;
            background: #fff;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            margin-bottom: 20px;
        }

        .counter {
            font-size: 24px;
            color: #333;
            font-weight: bold;
        }

        .icon {
            font-size: 40px;
            color: #007bff;
            margin-bottom: 10px;
        }


        .section-secondary-bg {
            background-color: #f8f9fa;
        }

        .section-custom-bg {
            background-size: cover;
            background-position: center;
            color: #fff;
        }

        .section-heading-wrapper h2 {
            margin-bottom: 20px;
        }

        .section-heading-wrapper h4 {
            color: #666;
        }

        .btn-theme {
            background-color: #007bff;
            color: #fff;
            border: none;
            padding: 10px 20px;
            border-radius: 5px;
            text-decoration: none;
        }

        .btn-theme-invert {
            background-color: #fff;
            color: #007bff;
            border: 1px solid #007bff;
        }

        .btn-small {
            padding: 5px 10px;
        }

        .heading-separator-horizontal {
            display: block;
            width: 50px;
            height: 2px;
            background: #007bff;
            margin: 10px auto;
        }

        .horizontal-newsletter input {
            padding: 10px;
            border: 1px solid #ddd;
            border-radius: 5px;
            width: 80%;
            margin-right: 10px;
        }

        .horizontal-newsletter button {
            padding: 10px 20px;
            border: none;
            border-radius: 5px;
            background-color: #007bff;
            color: #fff;
            cursor: pointer;
        }

        /* Back to Top Button */
        #backTop {
            position: fixed;
            bottom: 20px;
            right: 20px;
            background: #007bff;
            color: #fff;
            padding: 10px;
            border-radius: 5px;
            text-align: center;
            cursor: pointer;
            display: none;
        }


        .resultcontainer {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .row {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .column {
            flex-basis: 25%;
            margin: 0 2%
        }

        .card {
            background-color: #fff;
            border: 1px solid #ddd;
            border-radius: 10px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            padding: 20px;
            width: 100%;
        }

        .card-body {
            padding: 20px;
        }

        .card-title {
            font-size: 1.3em;
            font-weight: bold;
            margin-bottom: 10px;
        }

        .card-text {
            font-size: 1em;
            margin: 7% 4% 3% auto ;
            font-weight: bold;
        }

        .card-subtitle {
            font-size: 1.3em;
            font-weight: bold;
            margin-bottom: 10px;
        }

        .list-group {
            margin-bottom: 20px;
        }

        .list-group-item {
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

            .list-group-item.active {
                background-color: #337ab7;
                color: #fff;
            }

            .list-group-item:hover {
                background-color: #f5f5f5;
                cursor: pointer;
            }


        .table {
            border-collapse: collapse;
            width: 100%;
        }

            .table th, .table td {
                border: 1px solid #ddd;
                padding: 10px;
                text-align: left;
            }

            .table th {
                background-color: #f0f0f0;
            }

            .table tr:nth-child(even) {
                background-color: #f5f5f5;
            }

            .table tr:hover {
                background-color: #eee;
            }
        .section-heading-wrapper .text-left h2 {
            text-align: right;
            clear: both;
        }
        .section-heading-wrapper .text-left p, .section-heading-wrapper .text-left h4 {
            text-align: right;
        }

        .partycontainer {
            display: flex;
            gap: 10px;
            font-weight:400;
            flex-wrap: wrap;
            justify-content: center;
            direction:rtl !important;
        }
            .partycontainer p {
                font-size:1.2em;
                font-weight: 600;
            
            }
            .partycontainer .card {
                margin: 20px;
                padding: 20px;
                border: 1px solid #ddd;
                border-radius: 10px;
                box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
                width: calc(33.33% - 20px);
            }

            .partycontainer .card-header {
                background-color: #f0f0f0;
                padding: 10px;
                border-bottom: 1px solid #ddd;
            }

                .partycontainer .card-header h3, .card-header h4 {
                    margin: 0;
                }

        .partycontainer .card-body {
            padding: 20px;
        }

        .partycontainer .card-body ul {
            list-style: none;
            padding: 0;
            margin: 0;
        }

        .partycontainer .card-body li {
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

            .partycontainer .card-body li:last-child {
                border-bottom: none;
            }

        p {
            text-align:right !important;
            direction: rtl !important
        }
        
        .local_result {
            text-align: center;
            padding: 2% 3%;
            direction:rtl;
        }

        .date_container th{
            text-align:center !important;
            font-size:1.3em;
            font-weight:bolder;
        }
        .date_container td {
            font-weight: bold;
            text-align: center !important;
        }
        .theme-custom-box-animation {
            width: 70% !important;
            height: auto !important;
        }
        .date_container p{
            
        }
        .date_container {
            margin-left: 24% !important;
        
        }
        .adscontainer {
            width: 85% !important;
            height: 100vh !important;
        }

